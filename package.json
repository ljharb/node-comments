{
	"name": "comments",
	"version": "0.3.6",
	"description": "Transform between different comment styles",
	"main": "lib/comments.js",
	"bin": {
		"comments": "bin/cmd.js"
	},
	"preferGlobal": true,
	"scripts": {
		"test": "npm run lint && node test/index.js",
		"coverage": "covert test/*.js",
		"coverage-quiet": "covert test/*.js --quiet",
		"lint": "jscs */*.js"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/node-comments.git"
	},
	"keywords": [
		"promiseback",
		"comment",
		"comments",
		"html",
		"comments",
		"multiline",
		"//",
		"/*"
	],
	"author": "Jordan Harband",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/node-comments/issues"
	},
	"dependencies": {
		"extend": "~2.0.0",
		"listify": "~1.0.0",
		"literalizer": "~0.4.0-a",
		"optimist": "~0.6.1",
		"promiseback": "~2.0.0"
	},
	"devDependencies": {
		"tape": "~3.0.3",
		"covert": "1.0.0",
		"jscs": "~1.7.3"
	},
	"engines": {
		"node": ">= 0.4"
	}
}

